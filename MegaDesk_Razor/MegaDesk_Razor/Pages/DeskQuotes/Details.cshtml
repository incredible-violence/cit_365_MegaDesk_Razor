@page
@model MegaDesk_Razor.Pages.DeskQuotes.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-center" style="margin-bottom: 30px">Desk Quote Details</h1>
<hr />
<div class="col-md-offset-4 col-md-4">
    <dl class="row">
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.CustomerName)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.CustomerName)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.OrderDate)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.OrderDate)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.RushDays)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.RushDays)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.Drawers)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.Drawers)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.DeskMaterial)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.DeskMaterial)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.Width)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.Width)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.Length)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.Length)
        </dd>
        <dt class="col-sm-5">
            @Html.DisplayNameFor(model => model.DeskQuote.QuoteTotal)
        </dt>
        <dd class="col-sm-7">
            @Html.DisplayFor(model => model.DeskQuote.QuoteTotal)
        </dd>
    </dl>
    <div>
        <a asp-page="./Index" class="btn btn-primary">Back</a>
        <a asp-page="./Edit" asp-route-id="@Model.DeskQuote.DeskQuoteID" class="btn btn-primary">Edit</a>
    </div>
</div>
    
